        }
    ],
    "support": {
        "issues": "https://github.com/cakephp/cakephp/issues",
        "forum": "https://stackoverflow.com/tags/cakephp",
        "irc": "irc://irc.freenode.org/cakephp",
        "source": "https://github.com/cakephp/collection"
    },
    "require": {
        "php": "^5|^7.0",
        "ext-json": "*"
    },
    "require-dev": {
